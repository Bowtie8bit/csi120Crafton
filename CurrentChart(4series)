	}
	if (progressBar2->Value == 20) {
		label2->Text = "40%";
		label7->Text = "579";
		label13->Text = "2.338 Billion Metric Tons / year";
	}
	if (progressBar2->Value == 30) {
		label2->Text = "60%";
		label7->Text = "869";
		label13->Text = "3.510 Billion Metric Tons / year";
	}
	if (progressBar2->Value == 40) {
		label2->Text = "80%";
		label7->Text = "1,158";
		label13->Text = "4.678 Billion Metric Tons / year";
	}
	if (progressBar2->Value == 50) {
		label2->Text = "100%";
		label7->Text = "1,448";
		label13->Text = "5.848 Billion Metric Tons / year";
	}
	if (progressBar2->Value == 60) {
		label2->Text = "120%";
		label7->Text = "1,738";
		label13->Text = "7.019 Billion Metric Tons / year";
