// Cars (+)
private: System::Void button1_Click_1(System::Object^  sender, System::EventArgs^  e) {
	if (progressBar1->Value != 100) {
		progressBar1->Value += 10;
		series1->Points->Remove(dataPoint1);
		dataPoint1 = (gcnew System::Windows::Forms::DataVisualization::Charting::DataPoint(0, progressBar1->Value *2));
		series1->Points->Add(dataPoint1);
		series4->Points->Remove(dataPoint4);
		dataPoint4 = (gcnew System::Windows::Forms::DataVisualization::Charting::DataPoint(0, (progressBar1->Value + progressBar2->Value + progressBar3->Value) * 2 * .98));
		series4->Points->Add(dataPoint4);
		chartArea1->RecalculateAxesScale();
		chart1->Refresh();
		//
		if (TotalPollution->Value != 100)
			TotalPollution->Value += 2;
		//
	}

		// Percent, Quantity, & Emissions Update
		if (progressBar1->Value == 0) {
			label1->Text = "0%";
			label4->Text = "000.0";
			label12->Text = "000.0";
		}
		if (progressBar1->Value == 10) {
			label1->Text = "20%";
			label4->Text = "203.0 Million";
			label12->Text = "933.8 Million Metric Tons / year";
		}
		if (progressBar1->Value == 20) {
			label1->Text = "40%";
			label4->Text = "406.0 Million";
			label12->Text = "1.867 Billion Metric Tons / year";
		}
		if (progressBar1->Value == 30) {
			label1->Text = "60%";
			label4->Text = "609.0 Million";
			label12->Text = "2.801 Billion Metric Tons / year";
		}
		if (progressBar1->Value == 40) {
			label1->Text = "80%";
			label4->Text = "812.0 Million";
			label12->Text = "3.735 Billion Metric Tons / year";
		}
		if (progressBar1->Value == 50) {
			label1->Text = "100%";
			label4->Text = "1.015 Billion";
			label12->Text = "4.669 Billion Metric Tons / year";
		}
		if (progressBar1->Value == 60) {
			label1->Text = "120%";
			label4->Text = "1.218 Billion";
			label12->Text = "5.603 Billion Metric Tons / year";
		}
		if (progressBar1->Value == 70) {
			label1->Text = "140%";
			label4->Text = "1.421 Billion";
			label12->Text = "6.537 Billion Metric Tons / year";
		}
		if (progressBar1->Value == 80) {
			label1->Text = "160%";
			label4->Text = "1.624 Billion";
			label12->Text = "7.470 Billion Metric Tons / year";
		}
		if (progressBar1->Value == 90) {
			label1->Text = "180%";
			label4->Text = "1.827 Billion";
			label12->Text = "8.404 Billion Metric Tons / year";
		}
		if (progressBar1->Value == 100) {
			label1->Text = "200%";
			label4->Text = "2.030 Billion";
			label12->Text = "9.338 Billion Metric tons / year";
		}
	}
