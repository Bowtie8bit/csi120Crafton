	}

	// Percent, Quantity, & Emissions Update
	if (progressBar2->Value == 0) {
		label2->Text = "0%";
		label7->Text = "0";
		label13->Text = "000.0";
	}
	if (progressBar2->Value == 10) {
		label2->Text = "20%";
		label7->Text = "290";
