// Cows (+)
private: System::Void button6_Click(System::Object^  sender, System::EventArgs^  e) {
	if (progressBar3->Value != 100)
		progressBar3->Value += 10;
	series3->Points->Remove(dataPoint3);
	dataPoint3 = (gcnew System::Windows::Forms::DataVisualization::Charting::DataPoint(0, progressBar3->Value * 2));
	series3->Points->Add(dataPoint3);
	series4->Points->Remove(dataPoint4);
	dataPoint4 = (gcnew System::Windows::Forms::DataVisualization::Charting::DataPoint(0, (progressBar1->Value + progressBar2->Value + progressBar3->Value) * 2 * .98));
	series4->Points->Add(dataPoint4);
	chartArea1->RecalculateAxesScale();
	chart1->Refresh();

	//
	if (TotalPollution->Value != 100)
		TotalPollution->Value += 2;
	//
	// Percent Update
	if (progressBar3->Value == 0)
		label3->Text = "0%";
	if (progressBar3->Value == 10)
		label3->Text = "20%";
	if (progressBar3->Value == 20)
		label3->Text = "40%";
	if (progressBar3->Value == 30)
		label3->Text = "60%";
	if (progressBar3->Value == 40)
		label3->Text = "80%";
	if (progressBar3->Value == 50)
		label3->Text = "100%";
	if (progressBar3->Value == 60)
		label3->Text = "120%";
	if (progressBar3->Value == 70)
		label3->Text = "140%";
	if (progressBar3->Value == 80)
		label3->Text = "160%";
	if (progressBar3->Value == 90)
		label3->Text = "180%";
	if (progressBar3->Value == 100)
		label3->Text = "200%";
	// Quantity Update
	if (progressBar3->Value == 0)
		label8->Text = "0";
	if (progressBar3->Value == 10)
		label8->Text = "260 Million";
	if (progressBar3->Value == 20)
