private: System::Void button4_Click(System::Object^  sender, System::EventArgs^  e) {
	if (progressBar2->Value != 100) {
		progressBar2->Value += 10;
		series2->Points->Remove(dataPoint2);
		dataPoint2 = (gcnew System::Windows::Forms::DataVisualization::Charting::DataPoint(0, progressBar2->Value * 2));
		series2->Points->Add(dataPoint2);
		series4->Points->Remove(dataPoint4);
